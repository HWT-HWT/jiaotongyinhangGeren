.MYLoad {
  width: 100%;
  height: 100vh;
  background: linear-gradient(to bottom, #6b8cf5, #4775e4, #4775e4, #6b8cf5, #f7f7f7 35%);
}
.MYLoad .occupy {
  height: 100rpx;
}
.MYLoad .MyLoad-Navtitle {
  width: 90%;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #fff;
  font-weight: bold;
  font-family: 黑体;
}
.MYLoad .MyLoad-Navtitle image {
  width: 45rpx;
  height: 45rpx;
}
.MYLoad .LoadData {
  width: 90%;
  margin: 0 auto;
  margin-top: 40rpx;
}
.MYLoad .LoadData .LoadData-tow {
  width: 100%;
  display: flex;
  padding: 20rpx 0;
  align-items: center;
}
.MYLoad .LoadData .LoadData-tow .LoadData-tow-left {
  width: 50%;
  color: #c6dffe;
  font-size: 30rpx;
}
.MYLoad .LoadData .LoadData-tow .LoadData-tow-left ._p {
  display: flex;
  align-items: center;
}
.MYLoad .LoadData .LoadData-tow .LoadData-tow-left ._p image {
  width: 33rpx;
  height: 33rpx;
}
.MYLoad .LoadData .LoadData-tow .LoadData-tow-left .money {
  color: #fff;
  font-size: 40rpx;
  font-weight: bold;
  margin: 10rpx 0;
}
.MYLoad .LoadData .LoadData-footer {
  font-size: 30rpx;
  color: #fff;
  margin-top: 20rpx;
}
.MYLoad .MyLoad-datalist {
  width: 95%;
  padding-bottom: 40rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-title {
  width: 90%;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  padding: 30rpx 0;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-title .maxTitle {
  font-size: 35rpx;
  color: #000;
  font-weight: bold;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-title text {
  font-size: 28rpx;
  color: #6a8bf5;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-text {
  width: 90%;
  margin: 0 auto;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-text .one {
  font-size: 28rpx;
  color: #888;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-text .tow {
  font-size: 35rpx;
  font-weight: bold;
  margin: 15rpx 0;
}
.MYLoad .MyLoad-datalist .MyLoad-datalist-text ._span {
  margin-right: 40rpx;
  font-size: 30rpx;
  color: #999;
}

